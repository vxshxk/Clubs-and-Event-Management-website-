<div style="padding: 200px";>
<p style="color: green"><%= notice %></p>

<h1 class="title">Clubs</h1>

<div id="clubs" ,class="card">
  <% @clubs.each do |club| %>
    <%= render club %>
    <p>
      <%= link_to "Show this club", club, class:"card-footer-item button is-dark" %>
    </p>
    <br>
  <% end %>
  <br>
</div>
<% if !current_user.user? %>
   <%= link_to "New club", new_club_path ,class:"button is-info"%>
<% end %>
    <%= link_to "Back to Home", root_path, class:"button is-dark"%>
</div>